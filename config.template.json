{
  "outputDir": "./data",
  "tempDir": "./temp",
  "sources": {
    "archive": {
      "enabled": true,
      "baseUrl": "https://archive.org/advancedsearch.php",
      "concurrentDownloads": 5,
      "searchQueries": ["Manic Miners"],
      "dateRange": {
        "from": "2020-01-01",
        "to": "2024-12-31"
      }
    },
    "discord": {
      "enabled": true,
      "channels": ["https://discord.com/channels/YOUR_SERVER_ID/YOUR_CHANNEL_ID"]
    },
    "hognose": {
      "enabled": true,
      "githubRepo": "charredUtensil/hognose",
      "checkInterval": 86400000
    }
  }
}
{
  "name": "manic-miners-level-indexer",
  "version": "1.0.0",
  "description": "Unified indexing system for Manic Miners levels - combines archive scraping, Hognose indexing, map rendering, and catalog management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "lint": "eslint src scripts tests --ext .ts --fix",
    "lint:check": "eslint src scripts tests --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"scripts/**/*.{ts,js,json}\" \"tests/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"scripts/**/*.{ts,js,json}\" \"tests/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:discord": "ts-node tests/integration/test-discord.ts",
    "test:discord:small": "ts-node scripts/test/test-discord-small.ts",
    "test:hognose": "ts-node tests/integration/test-hognose.ts",
    "test:hognose:quick": "ts-node tests/integration/test-hognose-quick.ts",
    "test:internet-archive": "ts-node tests/integration/test-internet-archive.ts",
    "test:all": "ts-node tests/integration/test-all-indexers.ts",
    "test:analysis": "ts-node tests/integration/test-output-analysis.ts",
    "test:quick": "npm run test:internet-archive && npm run test:discord:small && npm run test:hognose:quick",
    "test:full": "ts-node scripts/run-full-test-suite.ts",
    "test:discord:auth": "ts-node scripts/test/test-discord-auth.ts",
    "verify:hognose": "ts-node tests/integration/verify-hognose-complete.ts",
    "index": "ts-node scripts/index-all.ts",
    "index:internet-archive": "ts-node scripts/index-internet-archive.ts",
    "index:discord:community": "ts-node scripts/index-discord-community.ts",
    "index:discord:archive": "ts-node scripts/index-discord-archive.ts",
    "index:hognose": "ts-node scripts/index-hognose.ts",
    "clean:test": "ts-node scripts/utils/clean-test-outputs.ts",
    "rebuild:catalog": "ts-node scripts/utils/rebuild-catalog.ts",
    "validate:catalog": "ts-node scripts/utils/validate-full-catalog.ts",
    "analyze:discord": "ts-node scripts/analyze-discord-channels.ts"
  },
  "keywords": [
    "manic-miners",
    "indexing",
    "scraping",
    "catalog",
    "maps"
  ],
  "author": "Wal33D",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "dotenv": "^17.2.0",
    "fs-extra": "^11.1.0",
    "node-fetch": "^2.7.0",
    "p-limit": "^3.1.0",
    "playwright": "^1.40.0",
    "progress": "^2.0.3",
    "unzipper": "^0.10.0",
    "xml2js": "^0.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.12",
    "@types/progress": "^2.0.0",
    "@types/unzipper": "^0.10.11",
    "@types/xml2js": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^16.3.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

{
  "name": "manic-miners-level-indexer",
  "version": "1.0.0",
  "description": "Unified indexing system for Manic Miners levels - combines archive scraping, Hognose indexing, map rendering, and catalog management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "lint": "eslint src scripts --ext .ts --fix",
    "lint:check": "eslint src scripts --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"scripts/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"scripts/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:example": "ts-node examples/basic-usage.ts",
    "test:archive": "ts-node test-archive.ts",
    "test:archive:improved": "ts-node test-improved-archive.ts",
    "test:archive:v2": "ts-node test-improved-archive-v2.ts",
    "test:discord": "ts-node test-discord.ts",
    "test:hognose": "ts-node test-hognose.ts",
    "test:all": "ts-node test-all-indexers.ts",
    "index": "ts-node scripts/index-all.ts",
    "index:archive": "ts-node scripts/index-archive.ts",
    "index:discord": "ts-node scripts/index-discord.ts",
    "index:hognose": "ts-node scripts/index-hognose.ts"
  },
  "keywords": [
    "manic-miners",
    "indexing",
    "scraping",
    "catalog",
    "maps"
  ],
  "author": "Manic Miners Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.53",
    "archiver": "^6.0.0",
    "canvas": "^2.11.2",
    "chalk": "^4.1.2",
    "fs-extra": "^11.1.0",
    "node-fetch": "^2.7.0",
    "p-limit": "^3.1.0",
    "playwright": "^1.40.0",
    "progress": "^2.0.3",
    "puppeteer": "^21.0.0",
    "sharp": "^0.32.0",
    "unzipper": "^0.10.0",
    "xml2js": "^0.6.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.12",
    "@types/progress": "^2.0.0",
    "@types/unzipper": "^0.10.11",
    "@types/xml2js": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
